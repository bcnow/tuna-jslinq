{
  "name": "tuna-jslinq",
  "version": "1.0.9",
  "description": "Linq methods for JavaScript/TypeScript for working with Arrays",
  "main": "dist/tuna-jslinq.js",
  "typings": "dist/typings/JSLinq.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/bcnow/tuna-jslinq.git"
  },
  "keywords": [
    "Linq",
    ".NET",
    "TypeScript",
    "JavaScript",
    "Array",
    "Sorting"
  ],
  "scripts": {
    "tsc": "tsc",
    "bundle": "tsc && rollup dist/JSLinq.js --file dist/tuna-jslinq.js --format umd --name \"JSLinq\"",
    "bundlemap": "tsc && rollup dist/JSLinq.js --file dist/tuna-jslinq.js --format umd --m dist/tuna-jslinq.js --name \"JSLinq\"",
    "build": "npm run tsc && npm run bundlemap"
  },
  "author": {
    "name": "bcnow"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bcnow/tuna-jslinq/issues"
  },
  "homepage": "https://github.com/bcnow/tuna-jslinq",
  "devDependencies": {
    "@types/jasmine": "2.8.6",
    "gulp": "3.9.1",
    "gulp-debug": "3.2.0",
    "gulp-jasmine": "3.0.0",
    "gulp-typescript": "5.0.0",
    "jasmine": "3.1.0",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-detect-browsers": "2.3.1",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "1.1.1",
    "typescript": "3.3.3"
  }
}